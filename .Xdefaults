! Set default font
!URxvt.font: xft:FuraCode Nerd Font:style=Regular:size=9
!URxvt.boldFont:xft:FuraCode Nerd Font:style=Regular:size=9

!URxvt.font: xft:SauceCodePro Nerd Font:style=Regular:size=9
!URxvt.boldFont:xft:SauceCodePro Nerd Font:style=Reqular:size=9

!URxvt.font: xft:MesloLGM Nerd Font:style=Regular:size=9
!URxvt.boldFont:xft:MesloLGM Nerd Font:style=Reqular:size=9

!URxvt.font: xft:FontAwesome:size=9
!URxvt.font: xft:FontAwesome Nerd Font:style=Regular:size=9

!URxvt.font: xft::DroidSans Nerd Font:style=Regular:size=9
!URxvt.boldFont:xft::DroidSans Nerd Font:style=Reqular:size=9

URxvt.font: xft:Knack Nerd Font:style=Regular:size=9
URxvt.boldFont: xft:Knack Nerd Font:style=Bold:size=9

! Display and configure scrollbar
URxvt*saveLines: 12000
URxvt*scrollBar: false
URxvt*scrollstyle: plain
URxvt*scrollBar_right: false
URxvt*scrollColor: #bfff00
! Set transparent background
URxvt*transparent: true
URxvt*shading:20

URxvt.letterSpace: 0
!######################### Colorscheme settings 
########################!
! Black baclground + Lightgray text
*background: #111111
*foreground: #babdb6
! Black + DarkGrey
*color0:  #000000
*color8:  #555753
! DarkRed + Red
*color1:  #c8031a
*color9:  #ff2828
! DarkGreen + Green
*color2:  #4e9a06
*color10: #93d44f
! DarkYellow + Yellow
*color3:  #eab93d
*color11: #ffc123
! DarkBlue + Blue
*color4:  #204a87
*color12: #5a8bcb
! DarkMagenta + Magenta
*color5:  #ce5c00
*color13: #f57900
!DarkCyan + Cyan
*color6:  #06989a
*color14: #46a4ff
! LightGrey + White
*color7:  #cccccc
*color15: #ffffff
